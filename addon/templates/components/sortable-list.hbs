{{#each items as |item index|}}
  {{#sortable-item
    item             = item
    items            = items
    group            = group
    handle           = handle
    debouncePeriod   = debouncePeriod
    draggingClass    = draggingItemClass
    classNameDefault = classNameItemDefault
  }}
    {{yield item index}}
  {{/sortable-item}}
{{/each}}